#!/usr/bin/env grrovy

pipeline {
   agent {  
         }
stages {
         stage('Build') {
	   steps {
		 echo "welcome to build evironament"
		 sh 'date'
		 } 
		
	     }
	  stage('Test') {
	     steps {
		   echo "welcome to build evironament"
		   sh cal
		   }
	  }
	 stage('Stage') {
	     steps {
           	    echo "welcome to build evironament"
		    sh 'cat /etc/passwd'
		   } 
	     }
     }   

}
